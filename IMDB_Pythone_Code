# group the data by genre and compute the average rating, average number of votes, and average revenue
genre_report = df1.groupby('Genre').agg({'Rating': 'mean', 'Votes': 'mean', 'Revenue_millions': 'mean'})

# print the genre-level report
genre_report = genre_report.round(2)

# print the genre report
print(genre_report)

# print the average rating of the 'Horror' genre
print("Average rating of the 'Horror' genre:", round(genre_report.loc['Horror', 'Rating'], 2))
