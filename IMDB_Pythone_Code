# Group the data by genre and year and calculate the total revenue for each group
genre_year_revenue = df1.groupby(['Genre', 'Year'])['Revenue_millions'].sum().reset_index()

# Rename the column to 'Total_revenue'
genre_year_revenue = genre_year_revenue.rename(columns={'Revenue_millions': 'Total_revenue'})

# Merge the total revenue dataframe with the original dataframe
merged_df = pd.merge(df1, genre_year_revenue, on=['Genre', 'Year'])

# Calculate the revenue of each movie as a percentage of the total revenue of its respective genre and year
merged_df['%_revenue'] = merged_df.apply(lambda row: 
                                          (row['Revenue_millions'] * 100) / row['Total_revenue'] 
                                          if row['Total_revenue'] != 0 
                                          else 0,
                                          axis=1)

# Preview the dataframe
print(merged_df.head())

# Query the dataframe for 'Split'
split_df = merged_df[merged_df['Title'] == 'Split']



# Print the % revenue of 'Split' in its respective genre and year
print("The % revenue of the movie 'Split' in its respective genre and year is:", round(split_df['%_revenue'].values[0], 2))
