# Group the data by year and compute the sum of revenue for each year
yearly_revenue = df1.groupby('Year')['Revenue_millions'].sum()

# Print the yearly revenue
print("Yearly revenue:")
print(yearly_revenue)

# Print the total revenue of all years
total_revenue = df1['Revenue_millions'].sum()
