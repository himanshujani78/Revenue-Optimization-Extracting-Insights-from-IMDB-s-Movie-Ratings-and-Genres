# Calculate and add the 'Votes_norm' column to the dataframe
df1['Votes_norm'] = df1.apply(lambda row: (row['Votes'] - df1['Votes'].min())*10 / (df1['Votes'].max() - df1['Votes'].min()), axis=1)

# Calculate the average 'Votes_norm'
avg_votes_norm = round(df1['Votes_norm'].mean(), 2)

# Print the result
print(f"The average 'Votes_norm' is: {avg_votes_norm}")
The average 'Votes_norm' is: 0.95
# Add the 'Total_rating' column to the dataframe
df1['Total_rating'] = df1.apply(lambda row: row['Rating'] + row['Votes_norm'], axis=1)

# Find the movie with the highest 'Total_rating'
highest_total_rating = df1['Total_rating'].max()

# Print the result
print(f"The highest 'Total_rating' is: {highest_total_rating}")
The highest 'Total_rating' is: 19.0
# Define the bin edges
bin_edges = [0, 50, 100, 150, df1['Revenue_millions'].max()]

# Define the bin labels
bin_labels = ['0-50', '51-100', '101-150', '150+']

# Create the 'Revenue_bins' column using cut() function
df1['Revenue_bins'] = pd.cut(df1['Revenue_millions'], bins=bin_edges, labels=bin_labels)

# Find the bucket with the highest number of movies
highest_bucket = df1['Revenue_bins'].value_counts().idxmax()

# Print the result
print(f"The bucket with the highest number of movies is '{highest_bucket}' with {df1['Revenue_bins'].value_counts().max()} movies.")
The bucket with the highest number of movies is '0-50' with 442 movies.
# Find the highest number of genres a director has created movies in
max_genres = df1.groupby('Director')['Genre'].nunique().max()

# Count the number of directors who have created movies in the highest number of genres
num_directors = (df1.groupby('Director')['Genre'].nunique() == max_genres).sum()

# Print the result
print(f"{num_directors} directors have created movies in the highest number of genres ({max_genres} genres).")
2 directors have created movies in the highest number of genres (5 genres).
# Find the highest number of genres a director has created movies in
max_genres = df1.groupby('Director')['Genre'].nunique().max()

# Find the directors who have created movies in the highest number of genres
directors_with_max_genres = df1.groupby('Director')['Genre'].nunique()[df1.groupby('Director')['Genre'].nunique() == max_genres]

# Print the result
print(f"{len(directors_with_max_genres)} directors have created movies in the highest number of genres ({max_genres} genres).")
print("\n")
print("Here are the names of the directors and the genres they have created movies in:")
print(directors_with_max_genres)
